<telerik:RadWindow x:Class="UniCloud.Presentation.Part.Report.MonthlyUtilizationReport"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
                   xmlns:silverlight="clr-namespace:Telerik.ReportViewer.Silverlight;assembly=Telerik.ReportViewer.Silverlight"
                   mc:Ignorable="d"
                   d:DesignHeight="300"
                   d:DesignWidth="400"
                   Height="600"
                   Width="800"
                   Header="Monthly Utilization Report">

    <Grid x:Name="LayoutRoot"
          Background="White">
        <silverlight:ReportViewer Name="ReportViewer"
                                  Height="558"
                                  HorizontalAlignment="Center"
                                  VerticalAlignment="Center"
                                  ReportServiceUri="../ReportService.svc"
                                  Report="ReportViewer.UtilizationReport.MonthlyUtilizationReport, ReportViewer, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
                                  Width="780" />
    </Grid>
</telerik:RadWindow>
